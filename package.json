{
  "name": "expense-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --max-warnings=0",
    "format:ci": "prettier --check .",
    "eslint:security:scan": "npx eslint . --ext js,ts --plugin security --config eslint.config.mjs",
    "ts:compilation:security:scan": "tsc --project tsconfig.security.json",
    "ts:tsec:security:scan": "tsec --project tsconfig.security.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "generate:branch:id": "sh ./scripts/generateBranchId.sh",
    "validate:branch:name": "sh ./scripts/validateBranchName.sh",
    "validate:commit:message": "sh ./scripts/validateCommitMessage.sh",
    "lint:staged": "sh ./scripts/lintStaged.sh",
    "start:docker:development": "sh ./scripts/startDockerDevelopment.sh",
    "create:docker:image": "sh ./scripts/createDockerImage.sh",
    "create:hash": "sh ./scripts/common/createHash.sh",
    "app:name": "sh ./scripts/common/appName.sh"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "branch-name-lint": "^3.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.11.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "^13.1.0",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "semantic-release": "^25.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsec": "^0.2.9",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
